{
  "name": "tetris-overload",
  "version": "1.0.0",
  "description": "A Tetris game made with React.",
  "main": "src/index.jsx",
  "private": true,
  "scripts": {
    "start": "webpack serve --config ./webpack.config.js --progress",
    "build": "npm run validate && cross-env NODE_ENV=production webpack --config webpack.config.js --progress",
    "build:nocheck": "cross-env NODE_ENV=production webpack --config webpack.config.js --progress",
    "serve": "http-server ./dist -g",
    "clean": "rm -rf node_modules && rm -rf dist",
    "eslint": "eslint --ext .jsx,.js ./",
    "stylelint": "stylelint ./src/**/*.{js,jsx}",
    "lint": "npm run eslint && npm run stylelint",
    "test": "jest --verbose",
    "test:watch": "jest --verbose --watch",
    "validate": "npm run lint && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lucasfrsi/tetris-overload"
  },
  "bugs": "https://github.com/lucasfrsi/tetris-overload/issues",
  "keywords": [
    "game",
    "react",
    "redux",
    "styled-components",
    "tetris"
  ],
  "author": "Lucas F. Simoni",
  "contributors": [
    {
      "name": "Lucas F. Simoni",
      "email": "lucasfrsi@gmail.com",
      "url": "https://lucasfrsi.com"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@testing-library/jest-dom": "^5.13.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "compression-webpack-plugin": "^8.0.0",
    "core-js": "^3.14.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.28.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^27.0.4",
    "jest-styled-components": "^7.0.4",
    "react-refresh": "^0.10.0",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.4",
    "redux": "^4.1.0",
    "redux-logger": "^3.0.6",
    "styled-components": "^5.3.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom",
      "jest-styled-components"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "^assets/(.*)$": "<rootDir>/src/assets/$1",
      "^components/(.*)$": "<rootDir>/src/components/$1",
      "^lib/(.*)$": "<rootDir>/src/lib/$1",
      "^hooks/(.*)$": "<rootDir>/src/hooks/$1",
      "^services/(.*)$": "<rootDir>/src/services/$1",
      "^store/(.*)$": "<rootDir>/src/store/$1",
      "^style/(.*)$": "<rootDir>/src/style/$1",
      "^utils/(.*)$": "<rootDir>/src/utils/$1"
    }
  }
}
